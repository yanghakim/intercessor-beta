    "bcrypt": "^5.0.0",
    "concurrently": "^4.1.2",
    "cookie-session": "^1.3.3",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "mongoose": "^5.6.13",
    "nodemailer": "^6.3.0",
    "nodemon": "^1.19.2",
    "passport": "^0.4.0",
